{
	"name": "react-ikonate",
	"version": "2.0.1",
	"description": "React version of fully customisable, accessible, well-optimised vector icons",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"es2015": "dist/es/index.js",
	"types": "dist/es/index.d.ts",
	"sideEffects": false,
	"keywords": [
		"icons",
		"outline",
		"customisable",
		"icon system",
		"generator",
		"accessibility"
	],
	"files": [
		"/dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/danielkov/react-ikonate.git"
	},
	"scripts": {
		"generate": "node scripts/generateReact.js",
		"build": "concurrently \"yarn build:es6\" \"yarn build:esm\" \"yarn build:cjs\"",
		"build:es6": "tsc --module es6 --target es6 --outDir dist/es",
		"build:esm": "tsc --module es6 --target es5 --outDir dist/esm",
		"build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
		"test": "jest",
		"test:coverage": "npm test -- --coverage",
		"test:badges": "npm run test:coverage  && jest-coverage-badges"
	},
	"author": "Mikolaj Dobrucki <tease@mikolajdobrucki.com>",
	"maintainers": [
		"Daniel Kovacs <kovacsemod@gmail.com>"
	],
	"license": "MIT",
	"devDependencies": {
		"@svgr/core": "4.3.3",
		"@svgr/plugin-jsx": "4.3.3",
		"@svgr/plugin-prettier": "4.3.2",
		"@svgr/plugin-svgo": "5.3.0",
		"@types/enzyme": "3.10.12",
		"@types/enzyme-adapter-react-16": "1.0.5",
		"@types/jest": "24.9.1",
		"@types/react": "16.9.22",
		"change-case": "3.1.0",
		"concurrently": "5.0.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.1",
		"enzyme-to-json": "3.4.3",
		"jest": "24.9.0",
		"microbundle": "0.11.0",
		"mkdirp": "0.5.1",
		"prettier": "1.19.1",
		"raf": "3.4.1",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"ts-jest": "24.1.0",
		"typescript": "3.8.3",
		"typings": "2.1.1"
	},
	"peerDependencies": {
		"react": "16.13.1",
		"react-dom": "16.13.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"\\.(ts|tsx)$": "ts-jest"
		},
		"setupFiles": [
			"raf/polyfill"
		],
		"testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
		"setupFilesAfterEnv": [
			"<rootDir>setupTests.ts"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"coverageReporters": [
			"json-summary",
			"text",
			"lcov"
		]
	}
}
